#
# NetrekII: Paradise Client Makefile
#

@SET_MAKE@

all:
	(cd src && $(MAKE))

static_images:
	(cd src && $(MAKE) static_images)

dynamic_images:
	(cd src && $(MAKE) dynamic_images)

unused_images:
	(cd src && $(MAKE) unused_images)

clean:
	(cd src && $(MAKE) clean)

distclean: clean
	(cd src && $(MAKE) distclean)
	-rm -f Makefile config.* include/config.h

cvsclean: distclean
	-rm -f configure
