#!/bin/sh
replace FFFF00000000 000078787878 < SC.xpm > SC2.xpm
replace FFFFFFFF0000 000078787878 < SC2.xpm > SC3.xpm
replace A9A9A9A9C0C0 0000C8C8C8C8 < SC3.xpm > SC4.xpm
replace 9A9A9A9AAFAF 0000A0A0A0A0 < SC4.xpm > SC5.xpm
replace 8D8D8D8DA0A0 000078787878 < SC5.xpm > SC6.xpm
replace C6C6C6C6E0E0 0000FFFFFFFF < SC6.xpm > SC7.xpm
replace B0B000000000 000078787878 < SC7.xpm > SC8.xpm
replace None '#000000000000' < SC8.xpm > SC9.xpm
